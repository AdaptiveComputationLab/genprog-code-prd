#!/bin/sh

# remove any old repairs and run with configuration2
cd gcd-multi-test && ../../src/repair ../gcd-multi-2-failstorepair.config

# check to see if we've generated a repair, pass if we have NOT
if [ -e repair.c ]
then
   ./clean.sh
   rm testruns.txt
   exit 1
else
   ./clean.sh
   rm testruns.txt
   exit 0
fi
~     
