  -> os:ASMOperands ai:ASMInputs 
            { let ins, clobs, count = ai in
               Some(REAL({aoutputs = fst os; ainputs = ins; aclobbers = clobs})), snd os + count }
  -> os:SPASMOperands ai:ASMInputs 
            { let ins, clobs, count = ai in
               Some(PART({aoutputsp = fst os; ainputsp = make_real_asm2 ins; aclobbersp = clobs})), 
                snd os + count }
  -> ai:ASMInputs 
            { let ins, clobs, count = ai in
               Some(REAL({aoutputs = []; ainputs = ins; aclobbers = clobs})), count }
  -> ai:SPASMInputs 
            { let ins, clobs, count = ai in
               Some(PART({aoutputsp = []; ainputsp = ins; aclobbersp = clobs})), count }
}

