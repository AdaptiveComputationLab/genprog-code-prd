nonterm(Cabs.partial_definition option * int) FPGlobalOpt {
  -> empty  { None, 0 }
  -> g:FPGlobal
            { Some(fst g), snd g }
}

nonterm(Cabs.partial_definition option * int) SPGlobalOpt {
  -> empty  { None, 0 }
  -> g:SPGlobal
            { Some(fst g), snd g }
}
nonterm(Cabs.partial_definition * int) FPGlobal {
  -> d:FPDeclaration
			{ d }
  -> e:FPExternDeclaration
 			{ e } 
  -> f:FPFunctionDefinition
			{ f }
  -> a:ASM LPAREN sc:StringConstant RPAREN
            { PARTGLOBASM(fst3 sc, a), trd3 sc + 3 }
  -> a:ASM LPAREN sc:StringConstant
            { PARTGLOBASM(fst3 sc, a), trd3 sc + 2 }
  -> a:ASM LPAREN
            { PARTGLOBASM("", a), 2 }
  -> a:ASM  { PARTGLOBASM("", a), 1 }
  -> p:FPPragma
  			{ p }
}

nonterm(Cabs.defp list * int) FPGlobals {
  -> g:FPGlobal 
            { [PART(fst g)], snd g }
  -> g:Global gs:FPGlobals
 			{ REAL(fst g) :: fst gs, snd g + snd gs }
  -> ";" gs:FPGlobals
			{ gs }
}

nonterm(Cabs.defp list * int) SPGlobals {
  -> g:SPGlobal gs:Globals
 			{ PART(fst g) :: make_real2 gs, snd g + snd gs }
}

nonterm(Cabs.partial_definition * int) SPGlobal {
  -> d:SPDeclaration
			{ d }
  -> e:SPExternDeclaration
 			{ e } 
  -> f:SPFunctionDefinition
			{ f }
  -> l:LPAREN sc:StringConstant RPAREN ";"
            { PARTGLOBASM(fst3 sc, l), trd3 sc + 3 }
  -> sc:StringConstant RPAREN ";"
            { PARTGLOBASM(fst3 sc, snd3 sc), trd3 sc + 2 }
  -> p:SPPragma
 			{ p }
}



nonterm(Cabs.partial_statement option * int) FPStatementOpt {
  -> empty  { None, 0 }
  -> s:FPStatement 
            { Some(fst s), snd s }
}

nonterm(Cabs.partial_statement option * int) SPStatementOpt {
  -> empty  { None, 0 }
  -> s:SPStatement 
            { Some(fst s), snd s }
}

