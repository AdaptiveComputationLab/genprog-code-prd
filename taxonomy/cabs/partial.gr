            { (fst lst), snd lst + 1 }
}

nonterm(Cabs.name_groupp list * bool * int) FPOldPardefList { 
  -> ds:DeclSpecList o:OldPardef ";"
            { [REAL(fst3 ds, fst o)], true, snd o + trd3 ds + 1 }
  -> ds:DeclSpecList o:OldPardef
            { [REAL(fst3 ds, fst o)], true, snd o + trd3 ds }
  -> ds:DeclSpecList o:FPOldPardef 
            { [PART(make_real ds, fst o)], true, snd o + trd3 ds }
  -> ds:DeclSpecList 
            { [PART(make_real ds, [])], false, trd3 ds }
  -> ds:FPDeclSpecList 
            { [PART(fst3 ds, [])], false, trd3 ds }
  -> ds:DeclSpecList o:OldPardef ";" lst:FPOldPardefList 
            { let rest, isva, count1 = lst in
                REAL(fst3 ds, fst o) :: rest, isva, trd3 ds + count1 + snd o + 1
            }
}

nonterm(Cabs.name_groupp list * bool * int) SPOldPardefList { 
  -> empty  { [], false, 0 }
  -> ds:SPDeclSpecList o:OldPardef ";" "..."
            { [PART(fst3 ds, make_real2 o)], true, snd o + trd3 ds + 2 }
  -> o:OldPardef ";" "..."
            { [REAL([], fst o)], true, snd o + 2 }
  -> o:SPOldPardef ";" "..."
            { [PART([], fst o)], true, snd o + 2 }
  -> ";" "..."
            { [], false, 2 }
  -> ds:SPDeclSpecList o:OldPardef ";" lst:OldPardefList 
            { let rest, isva, count1 = lst in
                 (PART(fst3 ds, make_real2 o) :: (make_real1 rest), isva, trd3 ds + count1 + snd o + 1) 
            }
  -> o:OldPardef ";" lst:OldPardefList 
            { let rest, isva, count1 = lst in
                 (REAL([], fst o) :: (make_real1 rest), isva, count1 + snd o + 1) 
            }
  -> o:SPOldPardef ";" lst:OldPardefList 
            { let rest, isva, count1 = lst in
                 (PART([], fst o) :: (make_real1 rest), isva,  count1 + snd o + 1) 
            }
  -> ";" lst:OldPardefList 
            { let rest, isva, count1 = lst in
                 EMPTY :: (make_real1 rest), isva, count1 + 1
            }
}

nonterm(Cabs.namep list * int) FPOldPardef {
  -> d:FPDeclarator
            { [fst d], snd d }
  -> d:Declarator "," o:FPOldPardef
