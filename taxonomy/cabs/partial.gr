  -> d:SPDeclaration seq:BlockElementList 
            { PART(PARTDEFINITION(fst d)) :: (make_real2 seq),  snd d + snd seq }
  -> ":"    { [ PART(PARTLABEL("", EMPTY, cabslu))], 1 }                    
}

